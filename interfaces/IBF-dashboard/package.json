{
  "name": "ibf-dashboard",
  "description": "Web interface for the dashboard of the IBF-system.",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "build": "ng build",
    "build:prod": "npm run build -- --configuration production",
    "compress": "gzipper compress --skip-compressed --remove-larger --gzip --gzip-level 9 ./www/",
    "e2e": "ng e2e",
    "format:fix": "npm run format:check -- --write",
    "format:check": "prettier --check \"**/*.{md,js,ts,scss,html}\" ",
    "lint:check": "ng lint",
    "lint": "npx npm-run-all --print-label --parallel lint:check format:check",
    "ng": "ng",
    "postbuild:prod": "npm run compress",
    "prebuild": "npm run set-env-variables",
    "prestart": "npm install --no-audit --no-fund",
    "serve:static": "cp www/index.html www/404.html && npx http-server www/ -c-1 --gzip",
    "set-env-variables": "node ./_set-env-variabes.js",
    "start": "ng serve --port=4200",
    "start:no-install": "ng serve --port=4200",
    "start:prod": "npm run build:prod && npm run serve:static",
    "test:dev": "ng test",
    "test:only": "ng test --no-watch --browsers=ChromeHeadless",
    "test": "npm run lint && npm run test:only",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "dependencies": {
    "@angular/common": "^18.1.0",
    "@angular/core": "^18.1.0",
    "@angular/forms": "^18.1.0",
    "@angular/platform-browser": "^18.1.0",
    "@angular/platform-browser-dynamic": "^18.1.0",
    "@angular/router": "^18.1.0",
    "@angular/service-worker": "^18.1.0",
    "@auth0/angular-jwt": "^5.0.1",
    "@bluehalo/ngx-leaflet": "^18.0.2",
    "@bluehalo/ngx-leaflet-markercluster": "^18.0.1",
    "@ionic/angular": "^6.7.5",
    "@microsoft/applicationinsights-web": "^2.5.11",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "@turf/center": "^6.0.1",
    "@turf/invariant": "^6.1.2",
    "dotenv": "^16.0.1",
    "gzipper": "^7.2.0",
    "leaflet": "^1.9.4",
    "leaflet.markercluster": "^1.5.3",
    "luxon": "^1.26.0",
    "rxjs": "~6.6.0",
    "tsconfig": "^7.0.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.14.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.1.0",
    "@angular-eslint/builder": "18.1.0",
    "@angular-eslint/eslint-plugin": "18.1.0",
    "@angular-eslint/eslint-plugin-template": "18.1.0",
    "@angular-eslint/schematics": "18.1.0",
    "@angular-eslint/template-parser": "18.1.0",
    "@angular/cli": "^18.1.0",
    "@angular/compiler": "^18.1.0",
    "@angular/compiler-cli": "^18.1.0",
    "@angular/language-service": "^18.1.0",
    "@cypress/schematic": "^2.4.0",
    "@ionic/cli": "^6.12.4",
    "@rollup/rollup-darwin-arm64": "^4.18.1",
    "@types/jasmine": "^5.1.4",
    "@types/leaflet": "^1.9.12",
    "@types/leaflet.markercluster": "^1.5.4",
    "@types/luxon": "^1.26.2",
    "@types/node": "^14.0.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "cypress": "latest",
    "esbuild": "^0.23.0",
    "eslint": "^8.57.0",
    "jasmine-core": "^5.1.2",
    "karma": "^6.4.3",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "prettier": "2.2.1",
    "prettier-plugin-organize-imports": "^2.3.4",
    "typescript": "^5.4.5",
    "xlsx": "^0.17.5"
  }
}
