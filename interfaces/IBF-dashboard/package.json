{
  "name": "ibf-dashboard",
  "description": "Web interface for the dashboard of the IBF-system.",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "build": "ng build",
    "build:prod": "npm run build -- --configuration production",
    "compress": "gzipper compress --skip-compressed --remove-larger --gzip --gzip-level 9 ./www/browser/",
    "e2e": "ng e2e",
    "fix": "ng lint --fix",
    "lint": "ng lint",
    "ng": "ng",
    "postbuild:prod": "npm run compress",
    "prebuild": "npm run set-env-variables",
    "prestart": "npm install --no-audit --no-fund",
    "serve:static": "cp www/browser/index.html www/browser/404.html && npx http-server www/browser/ -c-1 --gzip --port 4200",
    "set-env-variables": "node ./_set-env-variabes.js",
    "start": "ng serve",
    "start:no-install": "ng serve",
    "start:prod": "npm run build:prod && npm run serve:static",
    "test:dev": "ng test",
    "test": "npm run test:dev -- --no-watch --browsers=ChromeHeadless"
  },
  "dependencies": {
    "@angular/common": "^19.1.2",
    "@angular/core": "^19.1.2",
    "@angular/forms": "^19.1.2",
    "@angular/platform-browser": "^19.1.2",
    "@angular/platform-browser-dynamic": "^19.1.2",
    "@angular/router": "^19.1.2",
    "@angular/service-worker": "^19.1.2",
    "@auth0/angular-jwt": "^5.0.1",
    "@bluehalo/ngx-leaflet": "^19.0.0",
    "@bluehalo/ngx-leaflet-markercluster": "^19.0.0",
    "@ionic/angular": "^6.7.5",
    "@microsoft/applicationinsights-web": "^2.5.11",
    "@ngx-translate/core": "^16.0.4",
    "@ngx-translate/http-loader": "^8.0.0",
    "@turf/bbox": "^7.2.0",
    "@turf/invariant": "^7.2.0",
    "@turf/meta": "^7.2.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.0.1",
    "gzipper": "^8.2.0",
    "leaflet": "^1.9.4",
    "leaflet.markercluster": "^1.5.3",
    "luxon": "^3.5.0",
    "rxjs": "~7.8.1",
    "tsconfig": "^7.0.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.3",
    "@angular-eslint/builder": "19.1.0",
    "@angular-eslint/eslint-plugin": "19.1.0",
    "@angular-eslint/eslint-plugin-template": "19.1.0",
    "@angular-eslint/schematics": "19.1.0",
    "@angular-eslint/template-parser": "19.1.0",
    "@angular/cli": "^19.1.3",
    "@angular/compiler": "^19.1.2",
    "@angular/compiler-cli": "^19.1.2",
    "@angular/language-service": "^19.1.2",
    "@ionic/cli": "^7.2.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tanstack/eslint-plugin-query": "^5.61.3",
    "@types/jasmine": "^5.1.4",
    "@types/leaflet": "^1.9.12",
    "@types/leaflet.markercluster": "^1.5.4",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.14.10",
    "angular-eslint": "^19.0.2",
    "autoprefixer": "^10.4.7",
    "esbuild": "^0.25.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
