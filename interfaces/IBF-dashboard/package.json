{
  "name": "ibf-dashboard",
  "description": "Web interface for the dashboard of the IBF-system.",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "build": "ng build",
    "build:prod": "npm run build -- --prod",
    "e2e": "ng e2e",
    "fix:syntax": "npm run lint:syntax -- --write",
    "lint:syntax": "prettier --check \"**/*.{md,js,ts,scss,html}\" ",
    "lint": "npm run lint:syntax",
    "ng": "ng",
    "prebuild": "npm run set-env-variables",
    "prestart": "npm install --no-optional --no-audit --no-fund",
    "serve:static": "cp www/index.html www/404.html && npx http-server www/ -c-1",
    "set-env-variables": "node ./_set-env-variabes.js",
    "start": "ng serve --port=4200 --host=0.0.0.0 --disable-host-check",
    "start:no-install": "ng serve --port=4200 --host=0.0.0.0 --disable-host-check",
    "start:prod": "npm run build:prod && npm run serve:static",
    "test:dev": "ng test",
    "test:only": "ng test --no-watch --browsers=ChromeHeadless",
    "test": "npm run lint && npm run test:only",
    "postinstall": "ngcc"
  },
  "dependencies": {
    "@angular/common": "~10.0.3",
    "@angular/core": "~10.0.3",
    "@angular/forms": "~10.0.3",
    "@angular/platform-browser": "~10.0.3",
    "@angular/platform-browser-dynamic": "~10.0.3",
    "@angular/router": "~10.0.3",
    "@angular/service-worker": "~10.0.3",
    "@asymmetrik/ngx-leaflet": "^8.1.0",
    "@asymmetrik/ngx-leaflet-markercluster": "^5.0.1",
    "@auth0/angular-jwt": "^5.0.1",
    "@ionic/angular": "^5.2.3",
    "@turf/center": "^6.0.1",
    "@turf/invariant": "^6.1.2",
    "dotenv": "^8.2.0",
    "leaflet": "^1.6.0",
    "leaflet.markercluster": "^1.4.1",
    "moment": "^2.27.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.1",
    "@angular/cli": "~11.1.2",
    "@angular/compiler": "~10.0.3",
    "@angular/compiler-cli": "~10.0.3",
    "@angular/language-service": "~10.0.3",
    "@ionic/angular-toolkit": "^2.1.1",
    "@ionic/cli": "^6.10.1",
    "@types/jasmine": "~3.6.2",
    "@types/jasminewd2": "~2.0.3",
    "@types/leaflet": "^1.5.21",
    "@types/leaflet.markercluster": "^1.4.3",
    "@types/node": "^14.0.22",
    "codelyzer": "^6.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "^6.0.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "2.0.5",
    "prettier-plugin-organize-imports": "^1.1.1",
    "protractor": "~7.0.0",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.0",
    "typescript": "^3.9.6"
  }
}
