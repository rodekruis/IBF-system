{
  "name": "ibf-dashboard",
  "description": "Web interface for the dashboard of the IBF-system.",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "build": "ng build",
    "build:prod": "npm run build -- --prod",
    "e2e": "ng e2e",
    "format:fix": "npm run format:check -- --write",
    "format:check": "prettier --check \"**/*.{md,js,ts,scss,html}\" ",
    "lint:check": "ng lint",
    "lint": "npx npm-run-all --print-label --parallel lint:check format:check",
    "ng": "ng",
    "prebuild": "npm run set-env-variables",
    "prestart": "npm install --no-optional --no-audit --no-fund",
    "serve:static": "cp www/index.html www/404.html && npx http-server www/ -c-1",
    "set-env-variables": "node ./_set-env-variabes.js",
    "start": "ng serve --port=4200 --host=0.0.0.0 --disable-host-check",
    "start:no-install": "ng serve --port=4200 --host=0.0.0.0 --disable-host-check",
    "start:prod": "npm run build:prod && npm run serve:static",
    "test:dev": "ng test",
    "test:only": "ng test --no-watch --browsers=ChromeHeadless",
    "test": "npm run lint && npm run test:only",
    "postinstall": "ngcc --tsconfig \"tsconfig.json\""
  },
  "dependencies": {
    "@angular/common": "~11.1.1",
    "@angular/core": "~11.1.1",
    "@angular/forms": "~11.1.1",
    "@angular/platform-browser": "~11.1.1",
    "@angular/platform-browser-dynamic": "~11.1.1",
    "@angular/router": "~11.1.1",
    "@angular/service-worker": "~11.1.1",
    "@asymmetrik/ngx-leaflet": "^8.1.0",
    "@asymmetrik/ngx-leaflet-markercluster": "^5.0.1",
    "@auth0/angular-jwt": "^5.0.1",
    "@ionic/angular": "^5.2.3",
    "@microsoft/applicationinsights-web": "^2.5.11",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@turf/center": "^6.0.1",
    "@turf/invariant": "^6.1.2",
    "dotenv": "^16.0.1",
    "leaflet": "^1.6.0",
    "leaflet.markercluster": "^1.4.1",
    "luxon": "^1.26.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.2",
    "@angular/cli": "~14.2.1",
    "@angular/compiler": "~11.1.1",
    "@angular/compiler-cli": "~11.1.1",
    "@angular/language-service": "~11.2.3",
    "@ionic/angular-toolkit": "^3.1.0",
    "@ionic/cli": "^6.12.4",
    "@types/jasmine": "~3.6.2",
    "@types/jasminewd2": "~2.0.3",
    "@types/leaflet": "^1.5.21",
    "@types/leaflet.markercluster": "^1.4.3",
    "@types/luxon": "^1.26.2",
    "@types/node": "^14.0.22",
    "codelyzer": "^6.0.1",
    "jasmine-core": "~3.7.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "^6.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "2.2.1",
    "prettier-plugin-organize-imports": "^2.3.4",
    "protractor": "~7.0.0",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.0",
    "typescript": "^4.1.3 >=4.0.0 <4.2.0",
    "xlsx": "^0.17.5"
  }
}
