{
    "fields": {
        "name": {
            "type": "varchar",
            "required": true,
            "pattern": "$noBadCharacters"
        },
        "description": {
            "type": "text"
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "modifiedBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "assignedUser": {
            "type": "link",
            "required": false,
            "view": "views/fields/assigned-user"
        },
        "teams": {
            "type": "linkMultiple",
            "view": "views/fields/teams"
        },
        "subBasin": {
            "type": "enum",
            "required": true,
            "options": [
                "Sankarani",
                "Niger Supérieur",
                "Delta du Niger",
                "Niger Moyen",
                "Bani",
                "Baoulé",
                "Banifing",
                "Bagoé",
                ""
            ],
            "style": {
                "Sankarani": null,
                "Niger Supérieur": null,
                "Delta du Niger": null,
                "Niger Moyen": null,
                "Bani": null,
                "Baoulé": null,
                "Banifing": null,
                "Bagoé": null,
                "": null
            },
            "maxLength": 100,
            "isCustom": true
        },
        "latitude": {
            "notNull": false,
            "type": "float",
            "required": true,
            "min": 10,
            "max": 25,
            "isCustom": true
        },
        "longitude": {
            "notNull": false,
            "type": "float",
            "required": true,
            "min": -12,
            "max": 5,
            "isCustom": true
        },
        "altitude": {
            "notNull": false,
            "type": "float",
            "isCustom": true
        },
        "areaOfWatershed": {
            "notNull": false,
            "type": "float",
            "isCustom": true
        },
        "creationDate": {
            "type": "int",
            "min": 1900,
            "max": 2100,
            "disableFormatting": true,
            "isCustom": true,
            "tooltip": true
        },
        "twoyearReturnPeriod": {
            "type": "int",
            "isCustom": true
        },
        "fiveyearReturnPeriod": {
            "type": "int",
            "isCustom": true
        },
        "tenyearReturnPeriod": {
            "type": "int",
            "isCustom": true
        },
        "twentyyearReturnPeriod": {
            "type": "int",
            "isCustom": true
        },
        "communes": {
            "type": "linkMultiple",
            "layoutDetailDisabled": false,
            "layoutMassUpdateDisabled": false,
            "layoutListDisabled": false,
            "noLoad": false,
            "importDisabled": false,
            "exportDisabled": false,
            "customizationDisabled": false,
            "isCustom": true
        },
        "dischargeReportRecords": {
            "type": "linkMultiple",
            "layoutDetailDisabled": true,
            "layoutMassUpdateDisabled": true,
            "layoutListDisabled": true,
            "noLoad": true,
            "importDisabled": true,
            "exportDisabled": true,
            "customizationDisabled": true,
            "isCustom": true
        }
    },
    "links": {
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "assignedUser": {
            "type": "belongsTo",
            "entity": "User"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "entityTeam",
            "layoutRelationshipsDisabled": true
        },
        "communes": {
            "type": "hasMany",
            "foreign": "riverStation",
            "entity": "CCommunes",
            "audited": false,
            "isCustom": true
        },
        "dischargeReportRecords": {
            "type": "hasMany",
            "foreign": "riverStation",
            "entity": "CDischargeReportRecord",
            "audited": false,
            "isCustom": true
        }
    },
    "collection": {
        "orderBy": "name",
        "order": "desc",
        "textFilterFields": [
            "name"
        ],
        "fullTextSearch": false,
        "countDisabled": false
    },
    "indexes": {
        "name": {
            "columns": [
                "name",
                "deleted"
            ]
        },
        "assignedUser": {
            "columns": [
                "assignedUserId",
                "deleted"
            ]
        },
        "createdAt": {
            "columns": [
                "createdAt"
            ]
        },
        "createdAtId": {
            "unique": true,
            "columns": [
                "createdAt",
                "id"
            ]
        }
    },
    "optimisticConcurrencyControl": false
}