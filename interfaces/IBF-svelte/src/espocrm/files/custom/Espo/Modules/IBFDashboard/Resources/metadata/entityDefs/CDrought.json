{
    "fields": {
        "name": {
            "type": "varchar",
            "required": false,
            "pattern": "$noBadCharacters",
            "options": []
        },
        "description": {
            "type": "text"
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "modifiedBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "assignedUser": {
            "type": "link",
            "required": false,
            "view": "views/fields/assigned-user"
        },
        "teams": {
            "type": "linkMultiple",
            "view": "views/fields/teams"
        },
        "start": {
            "notNull": false,
            "type": "date",
            "isCustom": true,
            "tooltip": true
        },
        "end": {
            "notNull": false,
            "type": "date",
            "after": "start",
            "isCustom": true,
            "tooltip": true
        },
        "source": {
            "type": "text",
            "rowsMin": 2,
            "cutHeight": 200,
            "isCustom": true
        },
        "deaths": {
            "type": "int",
            "isCustom": true
        },
        "displaced": {
            "type": "int",
            "isCustom": true
        },
        "indirectlyAffected": {
