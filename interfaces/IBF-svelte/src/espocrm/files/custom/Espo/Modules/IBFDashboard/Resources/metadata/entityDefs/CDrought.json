    "links": {
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "assignedUser": {
            "type": "belongsTo",
            "entity": "User"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "entityTeam",
            "layoutRelationshipsDisabled": true
        },
        "meetings": {
            "type": "hasMany",
            "entity": "Meeting",
            "foreign": "parent",
            "layoutRelationshipsDisabled": true
        },
        "calls": {
            "type": "hasMany",
            "entity": "Call",
            "foreign": "parent",
            "layoutRelationshipsDisabled": true
        },
        "tasks": {
            "type": "hasChildren",
            "entity": "Task",
            "foreign": "parent",
            "layoutRelationshipsDisabled": true
        },
        "emails": {
            "type": "hasChildren",
            "entity": "Email",
            "foreign": "parent",
            "layoutRelationshipsDisabled": true
        },
        "branchs": {
            "type": "hasMany",
            "relationName": "cDroughtBranch",
            "foreign": "droughts",
            "entity": "CBranch",
            "audited": false,
            "isCustom": true
        },
        "regions": {
            "type": "hasMany",
            "relationName": "cDroughtRegion",
            "foreign": "droughts",
            "entity": "CRegion",
            "audited": false,
            "isCustom": true
        },
        "needsAssessments": {
            "type": "hasMany",
            "foreign": "drought",
            "entity": "CNeedsAssessment",
            "audited": false,
            "isCustom": true
        },
        "activities": {
            "type": "hasMany",
            "foreign": "drought",
            "entity": "CActivities",
            "audited": false,
            "isCustom": true
        },
        "cercles": {
            "type": "hasMany",
            "relationName": "cDroughtCercles",
            "foreign": "droughts",
            "entity": "CCercles",
            "audited": false,
            "isCustom": true
        },
        "communes": {
            "type": "hasMany",
            "relationName": "cDroughtCommunes",
            "foreign": "droughts",
            "entity": "CCommunes",
            "audited": false,
            "isCustom": true
        },
        "rapportEvaluationRapide": {
            "type": "belongsTo",
            "foreign": "drought",
            "entity": "CRapportEvaluationRapide",
            "isCustom": true
        },
        "hazard": {
            "type": "belongsTo",
            "foreign": "droughts",
            "entity": "CHazard",
            "audited": false,
            "isCustom": true
        }
    },
    "collection": {
        "orderBy": "createdAt",
        "order": "desc"
    },
    "indexes": {
        "name": {
            "columns": [
                "name",
                "deleted"
            ]
        },
        "assignedUser": {
            "columns": [
                "assignedUserId",
                "deleted"
            ]
        },
        "createdAt": {
            "columns": [
                "createdAt"
            ]
        },
        "createdAtId": {
            "unique": true,
            "columns": [
                "createdAt",
                "id"
            ]
        }
    }
}