            "layoutListDisabled": false,
            "noLoad": false,
            "importDisabled": false,
            "exportDisabled": false,
            "customizationDisabled": false,
            "isCustom": true
        },
        "regions": {
            "type": "linkMultiple",
            "layoutDetailDisabled": false,
            "layoutMassUpdateDisabled": false,
            "layoutListDisabled": false,
            "noLoad": false,
            "importDisabled": false,
            "exportDisabled": false,
            "customizationDisabled": false,
            "isCustom": true,
            "tooltip": true,
            "readOnly": false,
            "required": true
        },
        "needsAssessments": {
            "type": "linkMultiple",
            "layoutDetailDisabled": false,
            "layoutMassUpdateDisabled": false,
            "layoutListDisabled": false,
            "noLoad": false,
            "importDisabled": false,
            "exportDisabled": false,
            "customizationDisabled": false,
            "isCustom": true
        },
        "year": {
            "type": "varchar",
            "maxLength": 100,
            "options": [],
            "isCustom": true
        },
        "activities": {
            "type": "linkMultiple",
            "layoutDetailDisabled": false,
            "layoutMassUpdateDisabled": false,
            "layoutListDisabled": false,
            "noLoad": false,
            "importDisabled": false,
            "exportDisabled": false,
            "customizationDisabled": false,
            "isCustom": true
        },
        "cercles": {
            "type": "linkMultiple",
            "layoutDetailDisabled": true,
            "layoutMassUpdateDisabled": true,
            "layoutListDisabled": true,
            "noLoad": true,
            "importDisabled": true,
            "exportDisabled": true,
            "customizationDisabled": true,
            "isCustom": true
        },
        "communes": {
            "type": "linkMultiple",
            "layoutDetailDisabled": true,
            "layoutMassUpdateDisabled": true,
            "layoutListDisabled": true,
            "noLoad": true,
            "importDisabled": true,
            "exportDisabled": true,
            "customizationDisabled": true,
            "isCustom": true
