{
  "name": "ibf-api-service",
  "author": "IBF-system",
  "homepage": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint \"**/*.ts\"",
    "fix:syntax": "npm run lint:syntax -- --write",
    "lint:syntax": "prettier --check \"**/*.{md,js,ts,scss,html}\" ",
    "prestart": "npm run migration:run",
    "start": "node index.js",
    "prestart:dev": "npm run migration:run",
    "start:dev": "npx tsc-watch --onSuccess \"  node index.js  \" --onFailure \"echo There was a problem with the build!\" -p tsconfig.json",
    "prestart:prod": "tsc",
    "start:prod": "node dist/main.js",
    "seed": "npx ts-node src/scripts seed-init",
    "test": "jest --config=jest.json --detectOpenHandles --forceExit",
    "test:dev": "npm test -- --watchAll",
    "test:coverage": "npm test -- --coverage --coverageDirectory=coverage",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm migration:generate -- -n",
    "migration:run": "npm run typeorm migration:run"
  },
  "private": true,
  "dependencies": {
    "@godaddy/terminus": "^4.6.0",
    "@nestjs/common": "^6.11.11",
    "@nestjs/core": "^6.11.11",
    "@nestjs/microservices": "^6.11.11",
    "@nestjs/platform-express": "^6.11.11",
    "@nestjs/swagger": "^4.7.12",
    "@nestjs/terminus": "^6.5.6",
    "@nestjs/testing": "^6.11.11",
    "@nestjs/typeorm": "^6.3.4",
    "@nestjs/websockets": "^6.11.11",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.13.1",
    "crypto": "^1.0.1",
    "crypto-js": "^4.0.0",
    "cryptr": "^6.0.2",
    "csv-parser": "^3.0.0",
    "jsonwebtoken": "^8.1.1",
    "mailchimp-api-v3": "^1.15.0",
    "mysql": "^2.15.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "slug": "^4.0.2",
    "tsc-watch": "^2.4.0",
    "typeorm": "^0.2.30",
    "typescript": "^4.1.3",
    "wkt-io-ts": "^1.0.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.4",
    "@types/jest": "^26.0.20",
    "@types/node": "^12.19.16",
    "atob": ">=2.1.0",
    "deep-extend": ">=0.5.1",
    "eslint": "^5.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.3.1",
    "extend": ">=3.0.2",
    "jest": "^24.9.0",
    "nodemon": "^2.0.7",
    "prettier": "^1.19.1",
    "supertest": "^6.1.3",
    "swagger-ui-express": "^4.0.6",
    "ts-jest": "^24.3.0",
    "ts-node": "~9.1.1",
    "tsconfig-paths": "^3.9.0",
    "yargs": "^17.0.0"
  }
}
